meters":[]},{"code":101,"indent":0,"parameters":["\u30d5\u30a3\u30aa\u65b01",2,0,2]},{"code":401,"indent":0,"parameters":["Fio"]},{"code":401,"indent":0,"parameters":["\"Topia! Stay with me!\""]},{"code":101,"indent":0,"parameters":["\u30c8\u30d4\u30a21",5,0,2]},{"code":401,"indent":0,"parameters":["Topia"]},{"code":401,"indent":0,"parameters":["\"Fio...sama...  You came to help me...\""]},{"code":205,"indent":0,"parameters":[1,{"list":[{"code":41,"parameters":["Nature",4],"indent":null},{"code":18,"indent":null},{"code":0}],"repeat":false,"skippable":false,"wait":true}]},{"code":505,"indent":0,"parameters":[{"code":41,"parameters":["Nature",4],"indent":null}]},{"code":505,"indent":0,"parameters":[{"code":18,"indent":null}]},{"code":101,"indent":0,"parameters":["\u30d5\u30a3\u30aa\u65b01",3,0,2]},{"code":401,"indent":0,"parameters":["Fio"]},{"code":401,"indent":0,"parameters":["\"I'm sorry I'm late...\""]},{"code":101,"indent":0,"parameters":["\u30c8\u30d4\u30a21",2,0,2]},{"code":401,"indent":0,"parameters":["Topia"]},{"code":401,"indent":0,"parameters":["\"No, it's not Fio-sama's fault! "]},{"code":401,"indent":0,"parameters":["It's just that I'm unlucky...\""]},{"code":205,"indent":0,"parameters":[1,{"list":[{"code":16,"indent":null},{"code":0}],"repeat":false,"skippable":false,"wait":true}]},{"code":505,"indent":0,"parameters":[{"code":16,"indent":null}]},{"code":213,"indent":0,"parameters":[1,2,false]},{"code":101,"indent":0,"parameters":["\u30c8\u30d4\u30a21",0,0,2]},{"code":401,"indent":0,"parameters":["Topia"]},{"code":401,"indent":0,"parameters":["\"By the way, who might this be...?\""]},{"code":101,"indent":0,"parameters":["\u30d5\u30a3\u30aa\u65b01",0,0,2]},{"code":401,"indent":0,"parameters":["Fio"]},{"code":401,"indent":0,"parameters":["\"Ah, her name's Lena. For various reasons, "]},{"code":401,"indent":0,"parameters":["she's helping us take out the monsters.\""]},{"code":101,"indent":0,"parameters":["\u30ec\u30ca\u65b0\uff11",6,0,2]},{"code":401,"inden